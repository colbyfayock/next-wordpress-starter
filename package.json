{
  "name": "next-wordpress-starter",
  "homepage": "https://next-wordpress-starter.spacejelly.dev",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Colby Fayock <hello@colbyfayock.com>",
  "scripts": {
    "build": "next build && next export",
    "wpe-build": "npm run build",
    "dev": "next dev",
    "format": "yarn lint --fix",
    "format:no-path": "yarn lint:no-path --fix",
    "lint": "eslint .",
    "lint:no-path": "eslint",
    "start": "next start -p ${PORT:=3000}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": "yarn format:no-path",
    "**/*.scss": "yarn prettier --write"
  },
  "dependencies": {
    "@apollo/client": "^3.4.7",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "fabric": "^4.5.1",
    "fuse.js": "^6.4.6",
    "graphql": "^15.5.1",
    "he": "^1.2.0",
    "loader-utils": "^2.0.0",
    "next": "11.1.2",
    "next-compose-plugins": "^2.2.1",
    "parameterize": "^1.0.0",
    "path": "^0.12.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "rss": "^1.2.2",
    "sass": "^1.37.5",
    "style.css": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "4.3.8",
    "lint-staged": ">=11.1.2",
    "prettier": "2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/colbyfayock/next-wordpress-starter"
  },
  "bugs": {
    "url": "https://github.com/colbyfayock/next-wordpress-starter/issues"
  }
